			<div class="checkbox checkbox-styled">
				<label class="form-label">
					<input type="checkbox" class="ie" name="auto_login" data-type="use"
          value="ok" checked="checked">
					<span class="agree_tit">Stay Logined</span>
				</label>
			</div>
		</div>

		<p>
			<button class="btn btn-primary btn-block" style="font-size: 14px;">LogIn</button>
		</p>
	</form>


		<a href="#"
            class="float_l" id="signup">SignUp</a>
        <a href="#"
            onclick="SITE_MEMBER.openFindPassword('L2RpYWxvZy9sb2dpbi5jbQ%3D%3D');"
